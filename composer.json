{
    "name": "ecfectus/framework",
    "description": "The Ecfectus Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "leemason",
            "email": "contact@nohalfpixels.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "container-interop/container-interop": "^1.1",
        "zendframework/zend-diactoros": "~1.3@dev",
        "nikic/fast-route": "dev-master",
        "anahkiasen/underscore-php": "dev-master",
        "psr/http-message": "~1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "fabpot/php-cs-fixer": "~1.10"
    },
    "autoload": {
        "psr-4": {
            "Ecfectus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ecfectus\\Test\\": "tests"
        }
    },
    "provide": {
        "container-interop/container-interop-implementation": "^1.1"
    }
}
