{
    "name": "ecfectus/framework",
    "description": "The Ecfectus Framework",
    "license": "MIT",
    "homepage": "https://github.com/ecfectus/framework",
    "authors": [
        {
            "name": "leemason",
            "email": "contact@nohalfpixels.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "ecfectus/cache": "dev-master",
        "ecfectus/manager": "dev-master",
        "ecfectus/events": "dev-master",
        "ecfectus/dotable": "dev-master",
        "ecfectus/collection": "dev-master",
        "ecfectus/container": "dev-master",
        "ecfectus/pipeline": "dev-master",
        "symfony/http-foundation": "^3.1",
        "ecfectus/router": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.5.0",
        "phpdocumentor/phpdocumentor": "2.*",
        "fabpot/php-cs-fixer": "~1.10"
    },
    "autoload": {
        "psr-4": {
            "Ecfectus\\Framework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ecfectus\\Framework\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "docs": "vendor/bin/phpdoc"
    }
}
